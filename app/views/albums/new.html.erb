<div class="row">
  <fieldset>
    <legend>Upload Images</legend>
      <%= form_for [ @post, @album ], :html => {:multipart => true } do |f| %>
      <div class="field">
        <%= f.file_field :image, id: 'subm-field' %><br>
      </div>
    
        <%= f.submit "Add images", class: 'button tiny radius', id: 'subm', disabled: 'add images first' %>
    
      <% end %>
  </fieldset>
</div>